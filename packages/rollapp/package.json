{
    "name": "rollapp",
    "version": "0.0.0",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist"
    },
    "license": "MIT",
    "scripts": {
        "build": "tsup index.ts --format esm,cjs --dts --external react",
        "dev": "tsup index.ts --format esm,cjs --dts --external react --watch",
        "clean": "rm -rf dist",
        "gen:proto": "find ./proto -name 'tx.proto' | xargs protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --ts_proto_opt=esModuleInterop=true -I./proto -I ./proto/submodules/cosmos-proto/proto -I ./proto/submodules/protobuh/src/google/protobuf -I ./resolver/celestia/proto/submodules/gogoproto -I ./resolver/celestia/proto/submodules/googleapis -I ./third-party/ics23/proto -I ./proto/submodules/cosmos-sdk/proto -I ./proto/submodules/ibc-go/proto -I ./third-party/real-cosmos-sdk/proto",
        "gen:proto:blob": "find ./proto -name 'blob.proto' | xargs protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --ts_proto_opt=esModuleInterop=true -I./proto -I ./proto/submodules/cosmos-proto/proto -I ./proto/submodules/protobuh/src/google/protobuf -I ./resolver/celestia/proto/submodules/gogoproto -I ./resolver/celestia/proto/submodules/googleapis -I ./third-party/ics23/proto -I ./proto/submodules/cosmos-sdk/proto -I ./proto/submodules/ibc-go/proto -I ./third-party/real-cosmos-sdk/proto",
        "lint": "TIMING=1 eslint \"**/*.ts*\""
    },
    "dependencies": {
        "core": "*",
        "fetch": "*",
        "proto-utils": "*",
        "ts-proto": "^1.162.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/jest": "^29.5.3",
        "eslint": "^7.32.0",
        "eslint-config-custom": "*",
        "jest": "^29.6.2",
        "jest-environment-jsdom": "^29.6.2",
        "ts-jest": "^29.1.1",
        "tsconfig": "*",
        "tsup": "^6.1.3",
        "typescript": "^4.5.2"
    }
}
